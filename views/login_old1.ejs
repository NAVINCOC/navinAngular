  <body  style="background-image:url('images/hero-img9.jpg');">
    <div class="col-sm-12" style="max-height:400px;padding:5px; ">
      
      <div class="col-sm-6">        
        <h2 style="color:white">Polestar Solutions</h2>
      </div>
      <!-- login form -->
      <form   id="loginForm" method="post">
      <div class="col-sm-6">
      <!-- <div class="col-sm-12 height11"></div> -->
      <div class="col-sm-5">
        <div class="col-sm-12"><span class="front">Email</span></div>
        <div class="col-sm-12">
          <div class="input-group">
            <div class="input-group-addon"><i class="fa fa-envelope" aria-hidden="true"></i></div> 
             <input id="loginEmail" type="text" name="loginEmail" class="form-control " onfocusout="emailValidate('login')" value="<%=email%>"/></div>
          </div>
        <div class="col-sm-12"><span class="error" id="errorEmail"><%=error%></span></div>
      </div>
      <div class="col-sm-5">
        <div class="col-sm-12"><span class="front">Password</span></div>   
         <div class="col-sm-12">
          <div class="input-group">
            <div class="input-group-addon"><i class="fa fa-key" aria-hidden="true"></i></div> 
             <input type="Password" id="loginPassword" name="loginPassword" class="form-control" onfocusout="passwordValidate()"/></div>
          </div>
        <div class="col-sm-12 "><span class="error" id="errorPassword"></span></div>
      </div>
      <div class="col-sm-2">
        <div class="col-sm-12 height20"></div>
        <div class="col-sm-12">
          <input type="button" class="btn btn-primary form-control" name="Login" value="Login" onclick="validateLogin();" />
        </div>
      </div>
      </div>
      </form>
    </div>
<div id="mainpage" onclick="show()" >
  <div class="col-sm-12" style="height:650px;">
