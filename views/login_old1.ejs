        <div class="col-sm-12"><span class="error" id="errorEmailforget"></span></div>
      </div>
      
      <div class="col-sm-12 height20"></div>
        <div class="col-sm-12">
          <div class="col-sm-3"></div>
           <div class="col-sm-3">
          <input type="button" class="btn btn-primary form-control" name="Submit" value="Submit"/>
          </div>
          <div class="col-sm-3">
          <a class="btn btn-warning form-control" rel="modal:close" href="#">Cancel</a>
          </div>
        </div>
        <div class="col-sm-12 height11"></div>
        
    </div>     
  </div>

