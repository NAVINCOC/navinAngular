<!-- registeration modal -->
<div id="register-modal" style="display:none;width:900px;">
   
    <div class="col-sm-12 register-header">
      <span>Sign Up</span>
    </div>
    <div class="col-sm-12 register-body">

    <div class="col-sm-12">
    <form method="post" id="registerForm">
      <div class="col-sm-12 height25"></div>
      <div class="col-sm-12 height25"></div>
      <div class="col-sm-12">
        <div class="col-sm-4">
          <div class="col-sm-12"><span>Email</span></div>
            <div class="col-sm-12">
            <div class="input-group">
              <div class="input-group-addon"><i class="fa fa-envelope" aria-hidden="true"></i></div> 
             <input type="text" class="form-control" id="emailR" name="emailR" onfocusout="emailValidate('register')" value="<%=email%>"/></div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorEmailR"><%=error%></span></div>
      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Password</span></div>
        <div class="col-sm-12"><div class="input-group">
            <div class="input-group-addon"><i class="fa fa-key" aria-hidden="true"></i></div> 
             <input type="Password" class="form-control " id="passwordR" name="passwordR"/></div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorPasswordR"></span></div>
      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Confirm Password</span></div>
        <div class="col-sm-12">
            <div class="input-group">
              <div class="input-group-addon">
               <i class="fa fa-key" aria-hidden="true"></i>
              </div> 
             <input type="Password" class="form-control " id="confirmPasswordR" name="confirmPasswordR"/>
            </div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorConfirmPassword"></span></div>
      </div>
    </div>

   
    <div class="col-sm-12 height11"></div>
     <div class="col-sm-12">
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Address Line 1</span></div>
        <div class="col-sm-12">
        <div class="input-group">
              <div class="input-group-addon">
               <i class="fa fa-pencil" aria-hidden="true"></i>
              </div><input type="text" class="form-control" id="address1" name="address1"/></div>
              </div>
        <div class="col-sm-12"><span class="error" id="errorAddress"></span></div>
      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Address Line 2</span></div>
        <div class="col-sm-12">
        <div class="input-group">
              <div class="input-group-addon">
               <i class="fa fa-pencil" aria-hidden="true"></i>
              </div><input type="text" class="form-control" id="adress2" name="address2"/></div>
              </div>
      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Address Line 3</span></div>
        <div class="col-sm-12">
        <div class="input-group">
              <div class="input-group-addon">
               <i class="fa fa-pencil" aria-hidden="true"></i>
              </div>
              <input type="text" class="form-control" id="address3" name="address3"/></div>
              </div>

      </div>
    </div>  
    <div class="col-sm-12 height11"></div>
     <div class="col-sm-12">
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Country</span></div>
        <div class="col-sm-12">
          <div class="input-group">
            <div class="input-group-addon">
              <i class="fa fa-road" aria-hidden="true"></i>
            </div>
           <select class="form-control" id="country" name="country">
            <option value="0">Select</option>    
           </select>
           </div>
            </div>
           <div class="col-sm-12"><span class="error" id="errorCountry"></span></div>
       
        
      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>State</span></div>
        <div class="col-sm-12">
         <div class="input-group">
            <div class="input-group-addon">
              <i class="fa fa-road" aria-hidden="true"></i>
            </div>
          <select class="form-control" id="state" name="state">
            <option value="0">Select</option>    
           </select>
           </div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorState"></span></div>
      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>City</span></div>
        <div class="col-sm-12">
          <div class="input-group">
            <div class="input-group-addon">
              <i class="fa fa-road" aria-hidden="true"></i>
            </div>
          <select class="form-control" id="city" name="city">
            <option value="0">Select</option>    
           </select>
           </div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorCity"></span></div>
      </div>
    </div>  
     <div class="col-sm-12 height11"></div>
     <div class="col-sm-12">
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Pincode</span></div>
        <div class="col-sm-12">
          <div class="input-group">
            <div class="input-group-addon">
              <i class="fa fa-qrcode" aria-hidden="true"></i>
            </div>
          <select class="form-control" id="pincode" name="pincode">
            <option value="0">Select</option>  
            <option value="c">c</option>  
           </select>
           </div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorPincode"></span></div>
      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Contact No.</span></div>
        <div class="col-sm-12"> 
        <div class="input-group">
            <div class="input-group-addon">
              <i class="fa fa-phone" aria-hidden="true"></i>
            </div>
        <input type="text" class="form-control" id="contactno" name="contactno"/>
        </div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorContact"></span></div>
      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Contact Person</span></div>
        <div class="col-sm-12">
          <div class="input-group">
            <div class="input-group-addon">
              <i class="fa fa-user" aria-hidden="true"></i>
            </div>
          <input type="text" class="form-control" id="contactperson" name="contactperson"/>
          </div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorContactPerson"></span></div>
      </div>
    </div>  
     <div class="col-sm-12">
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Company Name</span></div>
        <div class="col-sm-12">
         <div class="input-group">
              <div class="input-group-addon">
               <i class="fa fa-bank" aria-hidden="true"></i>
              </div> 
             <input type="text" class="form-control " id="company" name="company"/>
            </div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorCompany"></span></div>
      </div>
      
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Industry Type</span></div>
        <div class="col-sm-12">
        <div class="input-group">
              <div class="input-group-addon">
               <i class="fa fa-bank" aria-hidden="true"></i>
              </div> 
             <select class="form-control" id="industryType" name="industryType">
           <option value="0">Select</option>
    
           </select>
            </div>
          
        </div>
        <div class="col-sm-12"><span class="error" id="errorIndustryType"></span></div>

      </div>
      <div class="col-sm-4">
        <div class="col-sm-12"><span>Company Type</span></div>
        <div class="col-sm-12">
          <div class="col-sm-6 padding-left0 padding-right0">
             <div class="col-sm-3 padding-left0 padding-right0"><input type="radio" id="companyRadio" name="companyType" value="Company" checked></div> <span>Company</span>
          </div>
          <div class="col-sm-6 padding-left0 padding-right0">
          <div class="col-sm-3 padding-left0 padding-right0"><input type="radio" id="consultant" name="companyType" value="Consultant"></div> <span>Consultant</span>
          </div>
        </div>
        <div class="col-sm-12"><span class="error" id="errorComapnyName"></span></div>
      </div>

    </div>
     <div class="col-sm-12 height20"></div>
      <div class="col-sm-12">
        <div class="col-sm-3"></div>
        <div class="col-sm-3 ">
          <input type="button" class="btn btn-primary form-control" name="Register Now" value="Register Now" onclick="validateRegidter()" />
        </div>
        <div class="col-sm-3">
          <a  class="btn btn-warning form-control" href="#" rel="modal:close" >Cancel</a>
        </div>
        <div class="col-sm-3"></div>
         <div class="col-sm-12 ">
         <div class="col-sm-12 height11"></div>
         <div class="col-lg-4"></div>
          <div class="col-sm-8 "><span style="color:#777 !important">Already Registered User Please </span><a href="#login_modal" rel="modal:open" class="btn-link" >Login</a> <span style="color:#777 !important">Here</span></div>
          </div>
      </div>
      </form>
      </div>
    </div>
</div>

<!-- End of Registeration modal -->

<!-- login modal  -->
<div id="login_modal" style="display:none;">
   
    <div class="col-sm-12 login-header">
    <span>Login</span>


    </div>
    <div class="col-sm-12 login-body">
       <div class="col-sm-12">
         
        <div class="col-sm-12"><span>Email</span></div>
        <div class="col-sm-12">
          <div class="input-group">
            <div class="input-group-addon"><i class="fa fa-envelope" aria-hidden="true"></i></div> 
             <input type="text" class="form-control " id="modalEmailLogin"/></div>
          </div>
        <div class="col-sm-12"><span class="error" id="errorEmailLogin"></span></div>
      </div>
       <div class="col-sm-12">

        <div class="col-sm-12"><span>Password</span></div>
        <div class="col-sm-12">
          <div class="input-group">
            <div class="input-group-addon"><i class="fa fa-key" aria-hidden="true"></i></div> 
             <input type="text" class="form-control" id="modelPasswordLogin"/></div>
          </div>
        <div class="col-sm-12"><span class="error" id="errorPasswordLogin"></span></div>
      </div>
      <div class="col-sm-12 height20"></div>
        <div class="col-sm-12">
          <div class="col-sm-3"></div>
           <div class="col-sm-3">
          <input type="submit" class="btn btn-primary form-control" name="Login" value="Login"/>
          </div>
          <div class="col-sm-3">
          <a class="btn btn-warning form-control" rel="modal:close" href="#">Cancel</a>
          </div>
        </div>
        <div class="col-sm-12 height11"></div>
        <div class="col-sm-12" id="forget">
        <center><span>Having Trouble in Login </span><a onclick="showmodalforget(); " href="#forget-modal" rel="modal:open" >Click here</a></center>
        </div>
    </div>     
  </div


<!-- Forget modal  -->
